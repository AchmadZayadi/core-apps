<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl3333"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="false">



            <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/rlUpper3333"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/text_color_light"
                app:layout_collapseMode="none"
                app:layout_scrollFlags="scroll|enterAlwaysCollapsed">


                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/ivCoverPhoto333"
                    android:layout_width="match_parent"
                    android:background="@color/placeholder_bg"
                    android:layout_height="@dimen/profile_cover_image"
                    android:scaleType="centerCrop" />




                <RelativeLayout
                    android:id="@+id/llAlbumImage33333333"
                    android:layout_width="@dimen/profile_main_image"
                    android:layout_height="@dimen/profile_main_image"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="@dimen/profile_cover_main_margin">

                    <View
                        android:id="@+id/thumbnail22"
                        android:layout_width="300dp"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/_10ssp"
                        android:layout_height="160dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="@dimen/margin_full"
                        android:background="@color/placeholder_bg" />



                </RelativeLayout>



                <View
                    android:id="@+id/thumbnail"
                    android:layout_width="300dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="@dimen/_15ssp"
                    android:layout_height="15dp"
                    android:layout_marginLeft="10dp"
                    android:layout_below="@+id/llAlbumImage33333333"
                    android:layout_marginRight="@dimen/margin_full"
                    android:background="@color/placeholder_bg" />


                <View
                    android:id="@+id/thumbnail2"
                    android:layout_width="200dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="@dimen/_10ssp"
                    android:layout_height="15dp"
                    android:layout_marginLeft="10dp"
                    android:layout_below="@+id/thumbnail"
                    android:layout_marginRight="@dimen/margin_full"
                    android:background="@color/placeholder_bg" />


                <View
                    android:id="@+id/thumbnail4"
                    android:layout_width="200dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="@dimen/_10ssp"
                    android:layout_height="20dp"
                    android:layout_marginLeft="10dp"
                    android:layout_below="@+id/thumbnail"
                    android:layout_marginRight="@dimen/margin_full"
                    android:background="@color/placeholder_bg" />

                <View
                    android:id="@+id/thumbnail5"
                    android:layout_width="200dp"
                    android:layout_marginTop="@dimen/_10ssp"
                    android:layout_height="20dp"
                    android:layout_marginLeft="10dp"
                    android:layout_below="@+id/thumbnail4"
                    android:layout_marginRight="@dimen/margin_full"
                    android:background="@color/placeholder_bg" />

                <View
                    android:id="@+id/thumbnail6"
                    android:layout_width="200dp"
                    android:layout_marginTop="@dimen/_10ssp"
                    android:layout_height="20dp"
                    android:layout_marginLeft="10dp"
                    android:layout_below="@+id/thumbnail5"
                    android:layout_marginRight="@dimen/margin_full"
                    android:background="@color/placeholder_bg" />

                <View
                    android:id="@+id/thumbnail70"
                    android:layout_width="200dp"
                    android:layout_marginTop="@dimen/_10ssp"
                    android:layout_height="20dp"
                    android:layout_marginLeft="10dp"
                    android:layout_below="@+id/thumbnail6"
                    android:layout_marginRight="@dimen/margin_full"
                    android:background="@color/placeholder_bg" />






                    <!--                    <View style="@style/divider" />-->

                    <RelativeLayout
                        android:id="@+id/rlRecent45"
                        style="@style/Forground_style"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"

                        android:layout_below="@+id/thumbnail70"
                        android:layout_marginTop="@dimen/margin_medium"
                        android:layout_marginBottom="@dimen/margin_half"
                        android:animateLayoutChanges="true"
                        android:visibility="gone"
                        tools:visibility="visible">


                        <View
                            android:id="@+id/thumbnail7"
                            android:layout_width="120dp"
                            android:layout_marginTop="@dimen/_10ssp"
                            android:layout_height="200dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="@dimen/margin_full"
                            android:background="@color/placeholder_bg" />

                        <View
                            android:id="@+id/thumbnail8"
                            android:layout_toEndOf="@+id/thumbnail7"
                            android:layout_width="120dp"
                            android:layout_marginTop="@dimen/_10ssp"
                            android:layout_height="200dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="@dimen/margin_full"
                            android:background="@color/placeholder_bg" />
                        <View
                            android:id="@+id/thumbnail9"
                            android:layout_toEndOf="@+id/thumbnail8"
                            android:layout_width="120dp"
                            android:layout_marginTop="@dimen/_10ssp"
                            android:layout_height="200dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="@dimen/margin_full"
                            android:background="@color/placeholder_bg" />

                    </RelativeLayout>

            </RelativeLayout>


 </androidx.coordinatorlayout.widget.CoordinatorLayout>

