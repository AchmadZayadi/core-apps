<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:emojicon="http://schemas.android.com/tools"
    android:id="@+id/rlDialogMain"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/_40ssp"
    android:theme="@style/AppThemeMatrial"
    android:background="@drawable/rounded_top_corner2"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_marginTop="@dimen/_20ssp"
        android:layout_marginLeft="@dimen/margin_half"
        android:layout_marginRight="@dimen/margin_half"
        android:layout_height="wrap_content">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvAlbumTitle_bt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nothing Like Spacial"
            android:textColor="@color/black_87"
            android:textSize="@dimen/sub_heading" />

            <ImageView
                android:id="@+id/canselid"
                android:layout_width="22sp"
                android:foregroundGravity="center_horizontal"
                android:layout_height="22sp"
                android:layout_alignParentEnd="true"
                android:rotation="45"
                android:src="@drawable/add_24dp"
                app:tint="@color/black"></ImageView>

    </RelativeLayout>



    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tvAlbumDetails_bt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_2ssp"
        android:text="2.7M Like | 105M View |  536 comment"
        android:layout_marginLeft="@dimen/margin_half"
        android:textColor="@color/black_87"
        android:textSize="@dimen/body_3" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="32sp"
        android:orientation="horizontal"
        >

        <androidx.appcompat.widget.LinearLayoutCompat
            android:id="@+id/llStar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/discritpionstid"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="@dimen/margin_half"
            android:layout_marginTop="@dimen/margin_quarter"
            android:layout_marginBottom="@dimen/margin_quarter">

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/ivStar1"
                android:layout_width="@dimen/size_star_song2"
                android:layout_height="@dimen/size_star_song2"
                android:layout_margin="@dimen/margin_star"
                app:srcCompat="@drawable/star_unfilled"
                app:tint="@color/ss_progress_startColor" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/ivStar2"
                android:layout_width="@dimen/size_star_song2"
                android:layout_height="@dimen/size_star_song2"
                android:layout_margin="@dimen/margin_star"
                app:srcCompat="@drawable/star_unfilled"
                app:tint="@color/ss_progress_startColor" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/ivStar3"
                android:layout_width="@dimen/size_star_song2"
                android:layout_height="@dimen/size_star_song2"
                android:layout_margin="@dimen/margin_star"
                app:srcCompat="@drawable/star_unfilled"
                app:tint="@color/ss_progress_startColor" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/ivStar4"
                android:layout_width="@dimen/size_star_song2"
                android:layout_height="@dimen/size_star_song2"
                android:layout_margin="@dimen/margin_star"
                app:srcCompat="@drawable/star_unfilled"
                app:tint="@color/ss_progress_startColor" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/ivStar5"
                android:layout_width="@dimen/size_star_song2"
                android:layout_height="@dimen/size_star_song2"
                android:layout_margin="@dimen/margin_star"
                app:srcCompat="@drawable/star_unfilled"
                app:tint="@color/ss_progress_startColor" />

        </androidx.appcompat.widget.LinearLayoutCompat>

        <TextView
            android:id="@+id/ratingtextid"
            android:layout_width="wrap_content"
            android:layout_height="17dp"
            android:layout_gravity="bottom"
            android:layout_marginLeft="@dimen/margin_half"
            android:fontFamily="@font/roboto_regular"
            android:layout_marginBottom="3sp"
            android:text="5.0 Rating"
            android:textSize="@dimen/_12ssp"></TextView>




    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_medium"
            android:text="Album:"
            android:textColor="@color/black_81"
            android:textSize="@dimen/headermusic"
            android:layout_marginLeft="@dimen/_8ssp"
            android:padding="@dimen/_4ssp"
            ></TextView>

        <TextView
            android:id="@+id/albumTitle_bt"
            android:layout_width="0dp"
            android:layout_weight="4"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_regular"
            android:text="Michel Jones"
            android:textColor="@color/black_81"
            android:textSize="@dimen/headermusic_des"
            android:layout_marginLeft="@dimen/_14ssp"
            android:padding="@dimen/_4ssp"
            ></TextView>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:visibility="gone"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_medium"
            android:text="Singer(s):"
            android:textColor="@color/black_81"
            android:textSize="@dimen/headermusic"
            android:layout_marginLeft="@dimen/_8ssp"
            android:padding="@dimen/_4ssp"
            ></TextView>

        <TextView
            android:id="@+id/albumsauthor"
            android:layout_width="0dp"
            android:layout_weight="4"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_regular"
            android:text="Jusite biber,The jakejones"
            android:textColor="@color/black_81"
            android:textSize="@dimen/headermusic_des"
            android:layout_marginLeft="@dimen/_14ssp"
            android:padding="@dimen/_4ssp"
            ></TextView>

    </LinearLayout>

    <LinearLayout
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_medium"
            android:text="Composer:"
            android:textColor="@color/black_81"
            android:textSize="@dimen/headermusic"
            android:layout_marginLeft="@dimen/_8ssp"
            android:padding="@dimen/_4ssp"
            ></TextView>

        <TextView
            android:layout_width="0dp"
            android:layout_weight="4"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_regular"
            android:text="Divine DHanuka"
            android:textColor="@color/black_81"
            android:textSize="@dimen/headermusic_des"
            android:layout_marginLeft="@dimen/_14ssp"
            android:padding="@dimen/_4ssp"
            ></TextView>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_medium"
            android:text="Posted By:"
            android:textColor="@color/black_81"
            android:textSize="@dimen/headermusic"
            android:layout_marginLeft="@dimen/_8ssp"
            android:padding="@dimen/_4ssp"
            ></TextView>

        <TextView
            android:id="@+id/postedby"
            android:layout_width="0dp"
            android:layout_weight="4"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_regular"
            android:text="Sandre Kingshaley"
            android:textColor="@color/black_81"
            android:textSize="@dimen/headermusic_des"
            android:layout_marginLeft="@dimen/_14ssp"
            android:padding="@dimen/_4ssp"
            ></TextView>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_medium"
            android:text="Create On:"
            android:textColor="@color/black_81"
            android:textSize="@dimen/headermusic"
            android:layout_marginLeft="@dimen/_8ssp"
            android:padding="@dimen/_4ssp"
            ></TextView>

        <TextView
            android:id="@+id/createdby"
            android:layout_width="0dp"
            android:layout_weight="4"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_regular"
            android:text="2021-08-06  4:31 PM"
            android:textColor="@color/black_81"
            android:textSize="@dimen/headermusic_des"
            android:layout_marginLeft="@dimen/_14ssp"
            android:padding="@dimen/_4ssp"
            ></TextView>

    </LinearLayout>




</androidx.appcompat.widget.LinearLayoutCompat>
