<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    >
<androidx.constraintlayout.widget.ConstraintLayout     xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/mainlayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appBar"
        android:layout_width="match_parent"
        android:theme="@style/AppThemeNoActionBarWelcome2"

        android:layout_height="@dimen/height_toolbar"
        tools:ignore="MissingConstraints">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:contentInsetStart="0dp"
            android:contentInsetLeft="0dp"
            android:contentInsetEnd="0dp"
            android:contentInsetRight="0dp"
            android:tag="@string/tag_toolbar"
            app:contentInsetEnd="0dp"
            app:contentInsetLeft="0dp"
            app:contentInsetRight="0dp"
            app:contentInsetStart="0dp"
            app:popupTheme="@style/AppThemeNoActionBarWelcome2">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:animateLayoutChanges="true">

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/ivBack"
                    style="@style/button_image_toolbar"
                    app:srcCompat="@drawable/arrow_left" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tvTitle"
                    style="@style/text_style_toolbar"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="48dp"
                    android:layout_toLeftOf="@+id/llOption"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:maxLines="1"
                    tools:text="@string/txt_sign_up"
                    android:textColor="@color/white"
                    android:textSize="@dimen/title" />

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:id="@+id/llOption"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:minWidth="48dp">

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/ivShare"
                        style="@style/button_image_toolbar"
                        android:visibility="gone"
                        app:srcCompat="@drawable/share_music"
                        tools:visibility="visible" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/ivOption"
                        style="@style/button_image_toolbar"
                        android:rotation="90"
                        android:visibility="gone"
                        app:srcCompat="@drawable/vertical_dots"
                        tools:visibility="visible" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/ivSearch"
                        style="@style/button_image_toolbar"
                        android:visibility="gone"
                        app:srcCompat="@drawable/ses_places_ic_search" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/ivSkip"
                        android:layout_width="wrap_content"
                        android:visibility="gone"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:layout_marginEnd="@dimen/_14ssp"
                        android:text="Skip"
                        android:textColor="@color/black"
                        android:textSize="18sp"
                        android:fontFamily="@font/helvetica_medium"

                        />


                </androidx.appcompat.widget.LinearLayoutCompat>
            </RelativeLayout>
        </androidx.appcompat.widget.Toolbar>
    </com.google.android.material.appbar.AppBarLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/ti1"
        android:layout_width="match_parent"
        app:layout_constraintTop_toBottomOf="@+id/appBar"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvLogin"
        android:layout_marginStart="@dimen/margin_full"
        android:layout_marginTop="@dimen/margin_24"
        android:layout_marginEnd="@dimen/margin_full"
        android:elevation="5dp"
        app:passwordToggleTint="@color/black"
        android:theme="@style/TextInputLayoutTheme2"
        tools:ignore="MissingConstraints">

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/etName"
            style="@style/Edit_text_style"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:hint="@string/name"
            android:inputType="textPersonName"
            android:maxLines="1"
            android:paddingStart="5dp"
            android:paddingTop="@dimen/margin_quarter"
            android:textColor="@color/black"
            android:textColorHint="@color/gray"
            android:textSize="@dimen/sub_heading" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/t21"
        android:layout_width="match_parent"
        app:layout_constraintTop_toBottomOf="@+id/ti1"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvLogin"
        android:layout_marginStart="@dimen/margin_full"
        android:layout_marginTop="@dimen/margin_full"
        android:layout_marginEnd="@dimen/margin_full"
        android:elevation="5dp"
        app:passwordToggleTint="@color/black"
        android:theme="@style/TextInputLayoutTheme2"
        tools:ignore="MissingConstraints">

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/etPhone"
            style="@style/Edit_text_style"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:hint="Phone"
            android:inputType="phone"
            android:maxLines="1"
            android:paddingStart="5dp"
            android:paddingTop="@dimen/margin_quarter"
            android:textColor="@color/black"
            android:textColorHint="@color/gray"
            android:textSize="@dimen/sub_heading" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/t24"
        android:layout_width="match_parent"
        app:layout_constraintTop_toBottomOf="@+id/t21"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvLogin"
        android:layout_marginStart="@dimen/margin_full"
        android:layout_marginTop="@dimen/margin_full"
        android:layout_marginEnd="@dimen/margin_full"
        android:elevation="5dp"
        app:passwordToggleTint="@color/black"
        android:theme="@style/TextInputLayoutTheme2"
        tools:ignore="MissingConstraints">

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/etEmail"
            style="@style/Edit_text_style"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:hint="Email"
            android:inputType="textEmailAddress"
            android:maxLines="1"
            android:paddingStart="5dp"
            android:paddingTop="@dimen/margin_quarter"
            android:textColor="@color/black"
            android:textColorHint="@color/gray"
            android:textSize="@dimen/sub_heading" />
    </com.google.android.material.textfield.TextInputLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/t24"
        android:layout_marginTop="@dimen/_20ssp"
        android:orientation="vertical">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/formElementButton"
            android:layout_below="@+id/tvForgotPassword"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginBottom="8dp"
            android:layout_weight="1"
            android:elevation="2dp"
            android:text=""
            android:padding="10dp"
            android:background="@drawable/button_signin"
            android:textAllCaps="false"
            android:textColor="#FFFFFF"
            android:textSize="20sp" />

    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>
</layout>