<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <declare-styleable name="MaxHeightScrollView">
        <attr name="maxHeight" format="dimension" />
    </declare-styleable>


    <declare-styleable name="FlowLayout">
        <attr name="android:gravity" />
    </declare-styleable>
    <declare-styleable name="FlowLayout_Layout">
        <attr name="android:layout_gravity" />
    </declare-styleable>

    <declare-styleable name="IconView">
        <attr name="reaction" format="enum">
            <enum name="like" value="0" />
            <enum name="love" value="1" />
            <enum name="laugh" value="2" />
            <enum name="wow" value="3" />
            <enum name="sad" value="4" />
            <enum name="angry" value="5" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SmallBangView">
        <attr name="dots_primary_color" format="color|reference" />
        <attr name="dots_secondary_color" format="color|reference" />
        <attr name="circle_start_color" format="color|reference" />
        <attr name="circle_end_color" format="color|reference" />
        <attr name="liked" format="boolean" />
        <attr name="anim_scale_factor" format="float" />
    </declare-styleable>

    <declare-styleable name="twowayview_TwoWayLayoutManager">
        <attr name="android:orientation" tools:ignore="ResourceName" />
    </declare-styleable>

    <declare-styleable name="twowayview_TwoWayView">
        <attr name="twowayview_layoutManager" format="string" />
    </declare-styleable>

    <declare-styleable name="twowayview_GridLayoutManager">
        <attr name="twowayview_numColumns" format="integer" />
        <attr name="twowayview_numRows" format="integer" />
    </declare-styleable>

    <declare-styleable name="twowayview_SpacingItemDecoration">
        <attr name="android:verticalSpacing" tools:ignore="ResourceName" />
        <attr name="android:horizontalSpacing" tools:ignore="ResourceName" />
    </declare-styleable>

    <declare-styleable name="twowayview_DividerItemDecoration">
        <attr name="android:divider" tools:ignore="ResourceName" />
        <attr name="twowayview_verticalDivider" format="reference" />
        <attr name="twowayview_horizontalDivider" format="reference" />
    </declare-styleable>

    <declare-styleable name="twowayview_StaggeredGridViewChild">
        <attr name="twowayview_span" format="integer" />
    </declare-styleable>

    <declare-styleable name="twowayview_SpannableGridViewChild">
        <attr name="twowayview_colSpan" format="integer" />
        <attr name="twowayview_rowSpan" format="integer" />
    </declare-styleable>


    <!--styles for custom FAB statrs-->
    <declare-styleable name="FloatingActionButton">
        <attr name="fabType" format="enum">
            <enum name="circle" value="0" />
            <enum name="square" value="1" />
            <enum name="roundedSquare" value="2" />
        </attr>
        <attr name="fabSizes" format="enum">
            <enum name="normal" value="10" />
            <enum name="mini" value="11" />
        </attr>
        <attr name="fabText" format="string" />
        <attr name="fabTextAllCaps" format="boolean" />
        <attr name="fabTextColor" format="color" />
        <attr name="fabElevation" format="dimension" />
        <attr name="fabColor" format="color" />
        <attr name="fabIcon" format="integer" />
        <attr name="fabIconColor" format="color" />
        <attr name="fabIconPosition" format="enum">
            <enum name="start" value="30" />
            <enum name="top" value="31" />
            <enum name="end" value="32" />
            <enum name="bottom" value="33" />
        </attr>
        <attr name="fabMenuStyle" format="enum">
            <enum name="popUp" value="20" />
            <enum name="popDown" value="21" />
            <enum name="popRight" value="22" />
            <enum name="popLeft" value="23" />
        </attr>
        <attr name="fabMenuGravity">
            <flag name="start" value="0" />
            <flag name="end" value="1" />
            <flag name="bottom" value="2" />
            <flag name="top" value="4" />
            <flag name="center_horizontal" value="8" />
            <flag name="center_vertical" value="16" />
            <flag name="center" value="31" />
        </attr>

    </declare-styleable>
    <!--styles for custom FAB ends-->

    <!--styles for CircularProgressbar ends-->
    <declare-styleable name="CircularProgressBar">
        <attr name="cpb_progress" format="integer" />
        <attr name="cpb_progressbar_color" format="color" />
        <attr name="cpb_background_progressbar_color" format="color" />
        <attr name="cpb_progressbar_width" format="dimension" />
        <attr name="cpb_background_progressbar_width" format="dimension" />
    </declare-styleable>
    <!--styles for CircularProgressbar ends-->

    <declare-styleable name="FlingLayout">
        <attr name="fl_isDragEnabled" format="boolean" />
        <attr name="fl_isDismissEnabled" format="boolean" />
    </declare-styleable>

    <!--styles for Pulse View starts-->
    <declare-styleable name="Pulsator4Droid">
        <attr name="pulse_count" format="integer" />
        <attr name="pulse_duration" format="integer" />
        <attr name="pulse_color" format="color" />
        <attr name="pulse_repeat" format="integer" />
        <attr name="pulse_maxScale" format="float" />
        <attr name="pulse_startFromScratch" format="boolean" />
        <attr name="pulse_interpolator" format="enum">
            <enum name="Linear" value="0" />
            <enum name="Accelerate" value="1" />
            <enum name="Decelerate" value="2" />
            <enum name="AccelerateDecelerate" value="3" />
        </attr>
    </declare-styleable>
    <!--styles for Pulse View ends-->
    <!--styles for SlideUpPanel starts-->
    <declare-styleable name="SlidingUpPanelLayout">
        <attr name="umanoPanelHeight" format="dimension" />
        <attr name="umanoShadowHeight" format="dimension" />
        <attr name="umanoParallaxOffset" format="dimension" />
        <attr name="umanoFadeColor" format="color" />
        <attr name="umanoFlingVelocity" format="integer" />
        <attr name="umanoDragView" format="reference" />
        <attr name="umanoScrollableView" format="reference" />
        <attr name="umanoOverlay" format="boolean" />
        <attr name="umanoClipPanel" format="boolean" />
        <attr name="umanoAnchorPoint" format="float" />
        <attr name="umanoInitialState" format="enum">
            <enum name="expanded" value="0" />
            <enum name="collapsed" value="1" />
            <enum name="anchored" value="2" />
            <enum name="hidden" value="3" />
        </attr>
        <attr name="umanoScrollInterpolator" format="reference" />
    </declare-styleable>

    <!--styles for PSlideUpPanel ends-->

    <!--Music Player attributs starts-->
    <attr name="icon_color" format="color" />
    <attr name="secondary_bg_color" format="color" />
    <attr name="theme_icon" format="color" />
    <!--Music Player attributs ends-->

    <!--Discreate Scrollview attributs starts-->
    <declare-styleable name="DiscreteScrollView">
        <attr name="dsv_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
    <!--Discreate Scrollview attributs ends-->

    <!--OTP view attributs starts-->
